#, kde-format
msgid "Process did not finish in reasonable time: %1 %2"
msgstr "Proces sa neukončil v rozumnom čase: %1 %2"

#: servicemenuinstaller.cpp:199
#, kde-format
msgid "Failed to uncompress %1"
msgstr "Zlyhala dekompresia %1"


msgid "Previous Tab"
msgstr "Əvvəlki Vərəq"

#: dolphinmainwindow.cpp:1756
#, kde-format
msgctxt "@action:inmenu"
msgid "Activate Previous Tab"
msgstr "Əvvəlki Vərəqə keçid"

